<script>
  function fetchTransactions(accountNumber, page) {
    let url = `/api/transactions/customers/${accountNumber}?page=${page}`
    fetch(url)
      .then(res => res.json())
      .then(data => console.log(data))
  }
  document.getElementById("btn-click").addEventListener('click', () => {
    fetchTransactions(3, 1);
  })
</script>
